/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode() : val(0), next(nullptr) {}
 *     ListNode(int x) : val(x), next(nullptr) {}
 *     ListNode(int x, ListNode *next) : val(x), next(next) {}
 * };
 */
class Solution {
public:
    ListNode* removeNthFromEnd(ListNode* head, int n) {
        int len=0;
        ListNode* temp; 
        if(head->next==NULL) return NULL;
        
        ListNode* first= head;
        ListNode* second=head;
        for(int i=0;i<n;i++){
            first=first->next;
        }
        while(!first){
            temp=head->next;
            delete(head);
            return temp;
        }
        while(first->next){
            first=first->next;
            second=second->next;
        }
        temp=second->next->next;
        delete(second->next);
        second->next=temp;
        
        
      
        return head;
    }
};
